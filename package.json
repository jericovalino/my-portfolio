{
  "name": "vite-template-solid",
  "version": "0.0.0",
  "description": "",
  "scripts": {
    "dev": "vite",
    "style:watch:": "postcss -w src/assets/styles/tailwindcss/index.css -o src/assets/styles/index.css",
    "style:build:": "postcss src/assets/styles/tailwindcss/index.css -o src/assets/styles/index.css",
    "solid:start": "vite",
    "solid:build": "vite build",
    "start": "npm-run-all -p style:watch solid:start",
    "build": "npm-run-all -s style:build solid:build",
    "serve": "vite preview"
  },
  "license": "MIT",
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.14",
    "postcss-cli": "^9.1.0",
    "tailwindcss": "^3.1.2",
    "typescript": "^4.6.4",
    "vite": "^2.9.9",
    "vite-plugin-solid": "^2.2.6"
  },
  "dependencies": {
    "postcss-import": "^14.1.0",
    "solid-app-router": "^0.3.3",
    "solid-icons": "^0.4.13",
    "solid-js": "^1.4.2"
  }
}
